{
    "domain": "example.com",
    "serverSubDomains": ["ns1", "ns2", "ns3"],
    "uiSubDomain": "ui.pektin",
    "apiSubDomain": "api.pektin",
    "vaultSubDomain": "vault.pektin",
    "recursorDomain": "recursor.pektin",

    "enableUi": true,
    "enableApi": true,
    "enableRecursor": true,

    "enableRotate": true,
    "createCerts": true,
    "letsencryptEmail": "letsencrypt@example.com",

    "autoConfigureMainDomain": true,

    "proxyConfig": "none",
    "createProxy": false,

    "buildFromSource": true,
    "dev": "local",
    "insecureDevIp": "1.1.1.1"
}
